{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/my_finance/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/my_finance/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/my_finance",
  "projects": {
    "account-svc": {
      "type": "application",
      "root": "apps/account-svc",
      "entryFile": "main",
      "sourceRoot": "apps/account-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/account-svc/tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-svc": {
      "type": "application",
      "root": "apps/auth-svc",
      "entryFile": "main",
      "sourceRoot": "apps/auth-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-svc/tsconfig.app.json"
      }
    },
    "my_finance": {
      "type": "application",
      "root": "apps/my_finance",
      "entryFile": "main",
      "sourceRoot": "apps/my_finance/src",
      "compilerOptions": {
        "tsConfigPath": "apps/my_finance/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "transaction-svc": {
      "type": "application",
      "root": "apps/transaction-svc",
      "entryFile": "main",
      "sourceRoot": "apps/transaction-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transaction-svc/tsconfig.app.json"
      }
    }
  }
}